#include <stdio.h>
 #include<string.h>
 int main()
{
    char username[100];
    int l,i,c=0;
  char k;
    scanf("%s",username);
    l=strlen(username);
    for(k='a';k<='z';k++)
    {
      for( i=0;i<l;i++)
        {
          if(username[i]==k)
            {
            c++;
            break;
            }
        }
    }
    if(c%2==0)
    printf("CHAT WITH HER!");
    else
    printf("IGNORE HIM!");
    return 0;
}
