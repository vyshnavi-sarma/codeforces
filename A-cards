#include <bits/stdc++.h>
using namespace std;
 
int main() {
    int n,i,l,countz=0,countn=0;
    cin>>n;
    string s;
cin>>s;
    for(i=0;i<n;i++){
        if(s[i]=='z'){
            countz++;
        }
        if(s[i]=='n'){
            countn++;
        }
    }
    for(i=0;i<countn;i++){
        cout<<1<<"\t";
    }
    for(i=0;i<countz;i++){
        cout<<0<<"\t";
    }}
    
