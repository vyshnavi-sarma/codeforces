#include<iostream>
#define ll long long
using namespace std;
int main(){
    string s;
    getline(cin,s);
    ll n=s.size();
    for (ll i =n-1; i>=0; i--)
    {
        if((65<=s[i] && s[i]<=90) || (97<=s[i] && s[i]<=122)){
 
            if(s[i]=='A' || s[i]=='E' || s[i]=='I' || s[i]=='O' || s[i]=='U' || s[i]=='Y'){
                cout<<"YES"; break;
            }
            else if(s[i]=='a' || s[i]=='e' || s[i]=='i' || s[i]=='o' || s[i]=='u' || s[i]=='y'){
                cout<<"YES"; break;
            }
            else{
                cout<<"NO"; break;
            }
        }
    }
    
    return 0;
}
