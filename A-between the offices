#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,i,sf,fs;
    string o;
    cin>>n;
    cin>>o;
    for( i=0; i<n-1; i++)
    {
        if(o[i]=='S' && o[i+1]=='F')
            sf+=1;
        else if(o[i]=='F' && o[i+1]=='S') 
            fs+=1;
    }
    if(sf>fs)
        cout<<"YES";
    else
        cout<<"NO";

    return 0;
}
